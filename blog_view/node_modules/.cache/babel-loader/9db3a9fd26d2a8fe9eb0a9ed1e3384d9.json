dth: 188px; float: left; margin-right: 10px\"\r\n        ></el-input>\r\n        <valid-code :value.sync=\"validCode\"></valid-code>\r\n      </el-form-item>\r\n      <el-tooltip class=\"item\" effect=\"dark\" content=\"登录后才能获得完整权限\" placement=\"top\">\r\n        <el-button @click=\"login('form')\">登录</el-button>\r\n      </el-tooltip>\r\n      <el-tooltip class=\"item\" effect=\"dark\" content=\"点击注册账号\" placement=\"top\">\r\n        <el-button @click=\"reginster\">注册</el-button>\r\n      </el-tooltip>\r\n      <el-tooltip class=\"item\" effect=\"dark\" content=\"游客模式下不可以评论和留言哦\" placement=\"top\">\r\n        <el-button @click=\"test\">游客访问</el-button>\r\n      </el-tooltip>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport ValidCode from '@/components/ValidCode.vue'\r\nexport default {\r\n  name: 'Login',\r\n  components: {\r\n    ValidCode,\r\n  },\r\n  data() {\r\n    return {\r\n      user: {\r\n        account: '',\r\n        password: '',\r\n        code: '',\r\n      },\r\n      rules: {\r\n        account: [\r\n          { required: true, message: '请输入账号', trigger: 'blur' },\r\n          {\r\n            min: 6,\r\n            max: 12,\r\n            message: '长度在 6 到 12 个字符',\r\n            trigger: 'blur',\r\n          },\r\n        ],\r\n        password: [\r\n          { required: true, message: '请输入密码', trigger: 'change' },\r\n          { min: 4, max: 8, message: '长度在 4 到 8 个字符', trigger: 'blur' },\r\n        ],\r\n        code: [{ required: true, message: '请输入验证码', trigger: 'change' }],\r\n      },\r\n      codeImgUrl: 'https://img0.baidu.com/it/u=3060508493,2267137751&fm=253&fmt=auto&app=138&f=JPEG?w=400&h=213',\r\n    }\r\n  },\r\n  methods: {\r\n    login(formName) {\r\n      let that = this\r\n      let userData = this.user\r\n      this.$refs[formName].validate((valid) => {\r\n        if (valid) {\r\n          alert('submit!')\r\n\r\n          that.$store\r\n            .dispatch('login', userData)\r\n            .then(() => {\r\n              that.$router.push({\r\n                path: '/',\r\n              })\r\n            })\r\n            .catch((error) => {\r\n              alert(error)\r\n              if (error !== 'error') {\r\n                that.$message({ message: '出错了', type: 'error', showClose: true })\r\n              }\r\n            })\r\n        } else {\r\n          console.log('error submit!!')\r\n          return false\r\n        }\r\n      })\r\n    },\r\n\r\n    test() {\r\n    },\r\n\r\n    reginster() {\r\n      this.$router.push({\r\n        path: '/reginster',\r\n      })\r\n    },\r\n\r\n    changeCodeImg() {},\r\n  },\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.formContainer {\r\n  position: absolute;\r\n  width: 100%;\r\n  height: 100%;\r\n  top: 0;\r\n  margin: 0 auto;\r\n  background-image: url('../assets/gif-bg1.gif');\r\n}\r\n\r\n.formContainer el-input {\r\n  background: linear-gradient(to right, #facabb);\r\n}\r\n\r\n.formContainer el-button {\r\n  background: linear-gradient(to right, #facabb);\r\n}\r\n\r\n.formContainer .loginForm {\r\n  border-radius: 15px;\r\n  background-clip: padding-box;\r\n  margin: 288px auto;\r\n  box-shadow: 0 0 10px gray;\r\n  width: 350px;\r\n  padding: 15px 35px 15px 35px;\r\n  border: 1px solid gray;\r\n}\r\n</style>\r\n"],"sourceRoot":"src/views"}]}