then(response => {\r\n          commit('SET_TOKEN', response.data)\r\n          console.log(response.data.data)\r\n          setUserData(response.data.data)\r\n          resolve()\r\n        }).catch(error => { \r\n          reject(error)\r\n        })\r\n      })\r\n    },\r\n\r\n    \r\n\r\n  }\r\n\r\n})\r\n"]}]}